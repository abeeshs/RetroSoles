<section class="product spad">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <ul class="filter__controls">

          <li data-filter=".new-arrivals">New Arrivals</li>
          <li data-filter=".hot-sales">Hot Sales</li>
        </ul>
      </div>
    </div>
    <div class="row product__filter">
      {{#each products}}
      <div class="col-lg-3 col-md-6 col-sm-6 col-md-6 col-sm-6 mix new-arrivals">
        <div class="product__item">
          <a class="" href="/products/view-product/{{this._id}}">
          <div class="product__item__pic set-bg" >
            <img src="{{this.urls.[0]}}" alt="">
            <span class="label">New</span>
            <ul class="product__hover">
              <li><a href="/add-to-wishlist/{{this._id}}"><img
                    src="data:image/png;base64,"
                    alt="" data-pagespeed-url-hash="935450538"></a></li>
              <li><a href="#"><i class="fa-thin fa-bag-shopping"></i> <span>Compare</span></a></li>
              {{!-- <li><a href="#"><img
                    src="data:image/png;base64,"
                    alt="" data-pagespeed-url-hash="3756334862"
                    onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></a></li> --}}
            </ul>
          </div>
            </a>
          <div class="product__item__text mt-5">
            <h6>{{this.product}}</h6>
            <a href="/add-to-cart/{{this._id}}" class="add-cart">+ Add To Cart</a>
            <div class="rating">
              <i class="fa fa-star-o active"></i>
              <i class="fa fa-star-o"></i>
              <i class="fa fa-star-o"></i>
              <i class="fa fa-star-o"></i>
              <i class="fa fa-star-o"></i>
            </div>
            <h5>Rs.{{this.discountprice}}</h5>
            
          </div>

        </div>
      </div>
      {{/each}}

      {{#each products}}
      <div class="col-lg-3 col-md-6 col-sm-6 col-md-6 col-sm-6 mix hot-sales">
        <div class="product__item">
          <a href="/products/view-product/{{this._id}}">
          <div class="product__item__pic set-bg" >
            <img src="{{this.urls.[1]}}" alt="" >
            <ul class="product__hover">
              <li><a href="#"><img
                    src="data:image/png;base64,"
                    alt="" data-pagespeed-url-hash="935450538"
                    onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></a></li>

            </ul>
          </div>
          <div class="product__item__text mt-5">
            <h6>{{this.product}}</h6>
            <a href="/add-to-cart/{{this._id}}" class="add-cart">+ Add To Cart</a>
            <div class="rating">
              <i class="fa fa-star-o"></i>
              <i class="fa fa-star-o"></i>
              <i class="fa fa-star-o"></i>
              <i class="fa fa-star-o"></i>
              <i class="fa fa-star-o"></i>
            </div>
            <h5>Rs.{{this.discountprice}}</h5>

          </div>
        </div>
      </div>
      {{/each}}
  
    </div>
  </div>
</section>


